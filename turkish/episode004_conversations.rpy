    # ra "I think so...{w} Yes."
    ra "Bence de...{w} Evet."

# game/episode004_conversations.rpy:854
translate turkish ep004_raene_talk_139c6bba:

    # c "Good."
    c "İyi."

# game/episode004_conversations.rpy:858
translate turkish ep004_raene_talk_80a44a2d:

    # c "Glad to have you aboard."
    c "Gemiye gittiğine sevindim."

# game/episode004_conversations.rpy:860
translate turkish ep004_raene_talk_9fc9cf0d:

    # c "I’ll be on the bridge if you need me."
    c "Bana ihtiyacın varsa köprüde olacağım."

# game/episode004_conversations.rpy:862
translate turkish ep004_raene_talk_f30b8018:

    # ra "Ah...{w} [p_name]?"
    ra "Ah...{w} [p_name]?"

# game/episode004_conversations.rpy:863
