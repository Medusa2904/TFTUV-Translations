    # ra "Thank you for taking us in."
    ra "Bizi içeri aldığınız için teşekkür ederim."

# game/episode004_conversations.rpy:908
translate turkish ep004_raene_talk_89cac49f:

    # c "It’s not trouble."
    c "Sorun değil."

# game/episode004_conversations.rpy:909
