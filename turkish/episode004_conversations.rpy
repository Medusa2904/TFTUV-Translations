    # ra "I'm sorry...{w} I..."
    ra "Üzgünüm...{w} İ..."

# game/episode004_conversations.rpy:923
translate turkish ep004_raene_talk_f97fd29a_1:

    # ra "I just can't."
    ra "Yapamam."

# game/episode004_conversations.rpy:925
translate turkish ep004_raene_talk_13d1c32c_1:

