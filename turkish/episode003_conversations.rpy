# game/episode003_conversations.rpy:266
translate turkish ep003_lilly_dialogue_0a143884:

    # c "I’ll monitor your life signals from the bridge."
    c "Yaşam sinyallerinizi köprüden izleyeceğim."

# game/episode003_conversations.rpy:268
translate turkish ep003_lilly_dialogue_7654248f:

    # l "Haha, you do that."
    l "Haha, bunu yap."

# game/episode003_conversations.rpy:269
translate turkish ep003_lilly_dialogue_dabbe7b3:

    # c "Night, Lilly."
    c "Gece, Lilly."

# game/episode003_conversations.rpy:270
translate turkish ep003_lilly_dialogue_bd95dbf0:

