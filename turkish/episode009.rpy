    # zi "[p_name]."
    zi "[p_name]."

# game/episode009.rpy:1770
translate turkish ep009_zi_grove_60bf5bc8:

    # c "Ziv."
    c "Ziv."

# game/episode009.rpy:1772
translate turkish ep009_zi_grove_63da90cf:

