# game/episode001.rpy:886
translate turkish ep001_basecamp_night_7cdb48d0:

    # c "I’m not really in the mood."
    c "Gerçekten havada değilim."

# game/episode001.rpy:887
translate turkish ep001_basecamp_night_6cb1d16d:

    # c "I’m going to rest for a while."
    c "Bir süre dinleneceğim."

# game/episode001.rpy:888
translate turkish ep001_basecamp_night_541ab0af:

    # j "I understand, you know where to find me."
    j "Anlıyorum, beni nerede bulacağınızı biliyorsun."

# game/episode001.rpy:890
translate turkish ep001_basecamp_night_d268016e_1:

    # "I fell into a deep slumber after Jade left, waking up hours later."
