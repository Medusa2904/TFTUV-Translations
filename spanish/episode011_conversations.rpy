translate spanish ep011_raene_talk_da2b5b5d:

    # ra "I'm quite okay, but the trip to those shipyards was nerve-wracking."
    ra "Yo estoy bastante bien, pero el viaje a esos astilleros fue angustioso."

# game/episode011_conversations.rpy:526
translate spanish ep011_raene_talk_8aec53f1:

    # c "It sure was."
    c "Sí que lo fue."

# game/episode011_conversations.rpy:528
