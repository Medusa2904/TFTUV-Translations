# game/prologue.rpy:28
translate german prologue_e98e362d:

    # cyborg "In the dying light of triple suns..."
    cyborg "Im sterbenden Licht von drei Sonnen..."

# game/prologue.rpy:29
translate german prologue_5881be7b:

    # cyborg "I hum a binary tune..."
    cyborg "Ich summe eine binäre Melodie..."

# game/prologue.rpy:30
translate german prologue_3a0fb19c:

