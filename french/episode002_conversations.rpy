    # t "Well, I’m fine."
    t "Et bien, je vais bien."

# game/episode002_conversations.rpy:382
translate french ep002_thim_talk_438e7968:

    # t "You can leave now."
    t "Peux tu t'en aller maintenant?"

# game/episode002_conversations.rpy:383
translate french ep002_thim_talk_3588663a:

    # c "Suit yourself."
    c "Comme tu voudras."

