    # t "Go the fuck away."
    t "Casse toi putain."

# game/episode002_conversations.rpy:379
translate french ep002_thim_talk_84b379f3:

    # t "Why are you here?"
    t "Que fais tu là?"

# game/episode002_conversations.rpy:380
translate french ep002_thim_talk_8d1860c5:

    # c "Just checking in."
    c "Je vérifie juste."

