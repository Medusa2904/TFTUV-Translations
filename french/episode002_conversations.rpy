    # t "Is it?"
    t "Vraiment?"

# game/episode002_conversations.rpy:362
translate french ep002_thim_talk_b14b4a40:

    # t "Didn’t make any difference."
    t "Cela n'a fait aucune différence."

# game/episode002_conversations.rpy:363
translate french ep002_thim_talk_64d0435d:

    # t "My older brother got her as his second attendant and treated her like shit."
    t "Mon frère aîné a eu la sienne en tant que second serviteur et il la traitait comme de la merde."

# game/episode002_conversations.rpy:364
translate french ep002_thim_talk_d3580a41:

    # c "That’s harsh."
    c "C'est rude."

# game/episode002_conversations.rpy:366
translate french ep002_thim_talk_adfadc38:

    # t "Yeah...{w} well..."
    t "Ouais..{w} bref..."

# game/episode002_conversations.rpy:369
translate french ep002_thim_talk_6e603eaa:

    # c "You’d better..."
    c "Tu aurais préféré..."

# game/episode002_conversations.rpy:371
translate french ep002_thim_talk_c3acb276:

    # t "Fuck you, what do you take me for?"
    t "Va te faire foutre, pour qui tu me prends?"

# game/episode002_conversations.rpy:372
translate french ep002_thim_talk_a1134ccd:

    # c "Never mind."
    c "Oublie ça."

# game/episode002_conversations.rpy:375
translate french ep002_thim_talk_16f55a2e:

    # t "Don't you fucking dare enter these quarters."
    t "Ne fouts pas tes pieds dans mes quartiers."

# game/episode002_conversations.rpy:376
