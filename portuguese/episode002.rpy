translate portuguese ep002_cargo_run_53cf4a3a:

    # "It turned out to be Thim, accompanied by two grim-faced warriors."
    "Aparentemente era Thim, acompanhando de dois guerreiros de cara feia."

# game/episode002.rpy:1242
translate portuguese ep002_cargo_run_7410065f:

