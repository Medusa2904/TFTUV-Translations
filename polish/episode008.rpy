    # ra "Okay."
    ra "Okay."

# game/episode008.rpy:1417
translate polish ep008_raene_baths_ebe96166:

    # c "Okay?"
    c "Okay?"

# game/episode008.rpy:1419
translate polish ep008_raene_baths_9af9e625:

