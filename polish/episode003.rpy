translate polish ep003_verdant_bar_conversations_9a60c2bb:

    # c "Nope."
    c "Nie."

# game/episode003.rpy:883
translate polish ep003_verdant_bar_conversations_2051358b:

    # th "So?"
    th "Więc?"

# game/episode003.rpy:885
