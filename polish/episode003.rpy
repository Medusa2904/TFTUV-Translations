translate polish ep003_verdant_bar_conversations_f1184471:

    # c "When did you plan on telling me?"
    c "Kiedy zamierzałaś mi powiedzieć?"

# game/episode003.rpy:902
translate polish ep003_verdant_bar_conversations_ff92942f:

    # th "The message came in minutes ago."
    th "Wiadomość nadeszła kilka minut temu."

# game/episode003.rpy:904
translate polish ep003_verdant_bar_conversations_24321ab8:

    # th "But it looks like we need another source of income."
    th "Wygląda na to, że potrzebujemy dodatkowego źródła dochodu."

